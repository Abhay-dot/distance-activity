#: activity/activity.info:2
msgid "Distance"
msgstr "Vzdialenosť"

#: activity.py:162
msgid "Atmosphere"
msgstr "Atmosféra"

#: activity.py:165
msgid "Custom metric"
msgstr "Bežný meter"

#: activity.py:187
msgid ""
"To measure the distance between two laptops, you must first share this "
"Activity."
msgstr ""
"Premerajte vzdialenosť medzi dvoma notebookmi, ako prvé musíte zdieľať \""
"Activity\"."

#: activity.py:189
msgid "Press the button to measure the distance to another laptop"
msgstr "Kliknite tlačidlo a premerajte vzdialenosť k ďalšiemu notebooku"

#: activity.py:191
msgid "Preparing to measure distance"
msgstr "Príprava na meranie vzdialenosti"

#: activity.py:192
msgid "Ready to make a measurement.  Waiting for partner to be ready."
msgstr "Pripravený na premeranie.   Čakanie na to, či je partner pripravený."

#: activity.py:194
msgid "Recording sound from each laptop."
msgstr "Nahrávanie zvuku z každého notebooku."

#: activity.py:195
msgid "Processing recorded audio."
msgstr "Spracovanie nahraného zvuku."

#: activity.py:197
msgid "This activity already has two participants, so you cannot join."
msgstr "Táto činnosť má už dvoch účastníkov, môžete sa pripojiť."

#: activity.py:200
msgid "Begin Measuring Distance"
msgstr "Začni merať vzdialenosť"

#: activity.py:201
msgid "Stop Measuring Distance"
msgstr "Ukonči meranie vzdialenosti"

#: activity.py:235 smoot_toolbar.py:99
#, python-format
msgid "Measured distance in %s"
msgstr "Odmeraná vzdialenosť v %s"

#: activity.py:235 smoot_toolbar.py:30 smoot_toolbar.py:34 smoot_toolbar.py:77
#: smoot_toolbar.py:100
msgid "meters"
msgstr "metre"

#: atm_toolbars.py:65
msgid "Temperature (C): "
msgstr "Teplota (° C): "

#: atm_toolbars.py:70
msgid "Relative Humidity (%): "
msgstr "Relatívna vlkosť (%): "

#: atm_toolbars.py:75
msgid "Speed of Sound (m/s): "
msgstr "Rýchlosť zvuku (m/s): "

#: calibration_toolbar.py:28
msgid "Calibration Offset (meters): "
msgstr "Nastavenie odchýlky (metre): "

#: smoot_toolbar.py:30 smoot_toolbar.py:35
msgid "centimeters"
msgstr "centimetre"

#. TRANS: English units of measure
#: smoot_toolbar.py:32 smoot_toolbar.py:36
msgid "inches"
msgstr "inches"

#: smoot_toolbar.py:32 smoot_toolbar.py:37
msgid "feet"
msgstr "stopa"

#: smoot_toolbar.py:32 smoot_toolbar.py:38
msgid "yards"
msgstr "yardy"

#: smoot_toolbar.py:33 smoot_toolbar.py:39
msgid "custom units"
msgstr "vlastné jednotky"

#: smoot_toolbar.py:81
msgid "Choose a unit of measure:"
msgstr "Vyberte jednotku miery:"

#: smoot_toolbar.py:84
msgid "select units"
msgstr "vybraná jednotka"

#: smoot_toolbar.py:103
#, python-format
msgid "%(unit)20.2f %(name)s per meter"
msgstr "%(unit)20.2f %(name)s na meter"
